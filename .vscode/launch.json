    "version": "0.2.0",
    "externalConsole":true ,
    "configurations": [
        {
            "name": "install package",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cocmd"
                ],
                "filter": {
                    "name": "cocmd",
                    "kind": "bin"
                }
            },
            "args": [
                "install"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "run",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cocmd"
                ],
                "filter": {
                    "name": "cocmd",
                    "kind": "bin"
                }
            },
            "args": [
                "--no-verbose",
                "run",
                "k8s.check-pod-restarts",
                "--param", "namespace=csp-app-dev",
                "--param", "regex = data"
                
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "profile-loader",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cocmd"
                ],
                "filter": {
                    "name": "cocmd",
                    "kind": "bin"
                }
            },
            "args": [
                "profile-loader"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "show packages",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cocmd"
                ],
                "filter": {
                    "name": "cocmd",
                    "kind": "bin"
                }
            },
            "args": [
                "show",
                "packages"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "show package",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cocmd"
                ],
                "filter": {
                    "name": "cocmd",
                    "kind": "bin"
                }
            },
            "args": [
                "show",
                "package",
                "prometheus"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "setup",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cocmd"
                ],
                "filter": {
                    "name": "cocmd",
                    "kind": "bin"
                }
            },
            "args": [
                "setup",
                
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "docs",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cocmd"
                ],
                "filter": {
                    "name": "cocmd",
                    "kind": "bin"
                }
            },
            "args": [
                "docs",
                "git",
                // "--raw-markdown",
                
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "tui",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cocmd"
                ],
                "filter": {
                    "name": "cocmd",
                    "kind": "bin"
                }
            },
            "args": [
                "browse"
                
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "howto",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cocmd"
                ],
                "filter": {
                    "name": "cocmd",
                    "kind": "bin"
                },
                "env": {
                    "LIBTORCH": "/opt/homebrew/Cellar/pytorch/2.0.1",
                    "LD_LIBRARY_PATH": "/opt/homebrew/Cellar/pytorch/2.0.1/lib:"
                }
            },
            "args": [
                "howto",
                "how to clean my docker env?"
                
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                "LIBTORCH": "/opt/homebrew/Cellar/pytorch/2.0.1",
                "LD_LIBRARY_PATH": "/opt/homebrew/Cellar/pytorch/2.0.1/lib:"
            }
        },
        {
            "name": "test cocmd-package",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=cocmd-package"
                ],
                "filter": {
                    "name": "cocmd-package",
                    "kind": "lib"
                }
            },
            "args": [ ],
            "cwd": "${workspaceFolder}"
        },
    ]
}